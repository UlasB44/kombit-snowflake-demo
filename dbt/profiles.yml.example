# KOMBIT dbt Profile
# Copy this to ~/.dbt/profiles.yml and update with your credentials

kombit_dbt:
  target: dev
  outputs:
    dev:
      type: snowflake
      account: your_account_identifier
      user: your_username
      authenticator: externalbrowser  # Or use password/key-pair
      role: SYSADMIN
      warehouse: COMPUTE_WH
      database: KOMBIT_C_DBT
      schema: STAGING
      threads: 4
